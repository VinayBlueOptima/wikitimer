{
    "name": "Wikipedia Timer",
    "description": "Avoid falling down the wikipedia rabbit hole!",
    "version" : "1.1",
    "manifest_version": 2,
    "permissions": ["storage",
                    "tabs",
                    "alarms",
                    "<all_urls>"],

    "background": {
        "persistent": true,
        "scripts": ["gatekeeper.js"]
    }
}
